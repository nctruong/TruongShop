<script type="text/javascript"> 
jQuery(document).ready(function($) {
  $(document).ready(function() {        
    $('.iosslider').iosSlider({
    snapToChildren: true,
    desktopClickDrag: true,
    infiniteSlider: true,
    snapSlideCenter: true,
    navNextSelector: $('.next'),
    navPrevSelector: $('.prev'),
    autoSlide: true,
    autoSlideTimer: 500000,
    autoSlideHoverPause: true,
    onSlideChange: slideChange,
    onSliderLoaded: slideChange
    });       
  }); 
        
  function slideChange(args) {
    $('#new_gallery').find('.active').removeClass('active');
    $(args.currentSlideObject).addClass('active');    
  }
});
function loadphoto(page,id){ 
    page = (!page) ? 1 : parseInt(page);
    id = (!id) ? 0 : parseInt(id);
    $("#loading").show();
    showid = '#showphotoajax';
    var reg = new RegExp('^\\d+$');
    if(reg.test(showid)) showid = '#showphotoajax';
    // get data chon loc  
    $.get(base_url + '/photo/ajaxlistphoto' , {page: page,id: id,limit: 24},function(data) {
        $(showid).html(data.html);
        $("#loading").hide();
    },'json');
}
