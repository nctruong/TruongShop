<script type="text/javascript">
//xem thêm mobile
var photoList = (function(){
  return {
    paging: function()
    {
      $("#to_be_continue_photo a").text('Đang tải');
      var htmlarea        =   $("#to_be_continue_photo a").attr('data-container');
      var category_id     =   $("#to_be_continue_photo a").attr('data-category');
      var page            =   $("#to_be_continue_photo a").attr('data-page');
      var url             =   $("#to_be_continue_photo a").attr('data-url');
      var param = {
        category_id: category_id,
        page: page
      };
      // post data
      $.post(base_url+url, param, function(data) {
      //console.log(data);
        if(data.error == 1)
        {
          $('#'+htmlarea).append(data.html);
          if(data.end == 1){
            $("#to_be_continue_photo").remove();
          }else{
            $("#to_be_continue_photo a").attr('data-page', data.page);
            $("#to_be_continue_photo a").text('Xem tiếp');
          }
        }
        else if(data.error == 2)
        {
          alert(data.message);
          $("#to_be_continue_photo").remove();
        }
        else
        {
          alert('Server dang bận, vui lòng thử lại vào lúc khác');
          $("#to_be_continue_photo").remove();
        }
      },"json");
    }
  }
})();
$(function(){
   $( "#to_be_continue_photo a" ).bind( "click", function() {
    photoList.paging();
  });
});
</script>      </div>
